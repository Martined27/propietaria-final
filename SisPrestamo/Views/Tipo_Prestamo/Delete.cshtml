@model SisPrestamo.tipo_prestamo

@{
    ViewBag.Title = "Eliminar";
}

<h2>Eliminar</h2>

<h3>Esta seguro que desea eliminar esto?</h3>
<fieldset>
    <legend>Tipo Préstamo</legend>

    <div class="display-label">
